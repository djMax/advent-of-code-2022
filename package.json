{
  "name": "@gasbuddy/advent-of-code-2022",
  "version": "1.0.0",
  "description": "Solutions for the Advent of Code 2022",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "clean": "yarn dlx rimraf ./build",
    "prepare": "husky install",
    "lint": "eslint src",
    "postinstall": "yarn dlx -p @gasbuddy/coconfig -p coconfig coconfig"
  },
  "config": {
    "coconfig": "@gasbuddy/coconfig"
  },
  "engines": {
    "node": ">18.0.0"
  },
  "author": "GasBuddy Developers <developers@gasbuddy.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gas-buddy/advent-of-code-2022.git"
  },
  "keywords": [
    "typescript",
    "gasbuddy"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint --cache --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4.14.191",
    "eslint": "^8.2.0",
    "eslint-config-gasbuddy": "^7.0.2",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-openapi": "^0.14.2",
    "lint-staged": "^13.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@gasbuddy/coconfig": "^1.0.0",
    "lite-pathfindings": "^1.1.3",
    "lodash": "^4.17.21",
    "threads": "^1.7.0"
  },
  "packageManager": "yarn@3.3.0"
